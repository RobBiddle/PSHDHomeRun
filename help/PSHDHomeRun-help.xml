    <command:details>
      <command:name>Remove-HDHomeRunRecording</command:name>
      <command:verb>Remove</command:verb>
      <command:noun>HDHomeRunRecording</command:noun>
      <maml:description>
        <maml:para>Removes a recording from the HDHomeRun DVR. Very fast! Be careful!</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
